<odoo>
    <data>
        <template id="data_view_template" inherit_id="web.assets_backend">
            <xpath expr="//div[@class='o_sub_menu']" position="before">
                <div class="o_data_view">
                    <h1>Table 1 Data</h1>
                    <ul>
                        <t t-foreach="table1_data" t-as="data">
                            <li><t t-esc="data.libelle"/></li>
                        </t>
                    </ul>
                    <h1>Table 2 Data</h1>
                    <ul>
                        <t t-foreach="table2_data" t-as="data">
                            <li><t t-esc="data.libelle"/></li>
                        </t>
                    </ul>
                </div>
            </xpath>
        </template>
    </data>
</odoo>
